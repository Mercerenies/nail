
// TODO Change numbering if we cut endings

function endingTagline(ending) {
  switch (ending) {
  case Ending.PRISON:
    return "Ending 1 / 7: Do Not Pass Go";
  case Ending.RING:
    return "Ending 2 / 7: This Thing of Ours";
  case Ending.DARKNESS:
    return "Ending 3 / 7: Master of the Dark Arts";
  case Ending.TYCOON:
    return "Ending 4 / 7: Richest Man on Earth";
  case Ending.FAILURE:
    return "Ending 5 / 7: Luigi Wins";
  case Ending.SATAN:
    return "Ending 6 / 7: A Friend Down Below";
  case Ending.BADGE:
    return "Ending 7 / 7: New Sheriff in Town";
  }
}
